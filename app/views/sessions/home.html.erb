<h1>Training Journal</h1>

<%= form_for :user do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <br>
    <%= f.submit 'Login' %>
    <% end %>
    <br>
    <%= link_to 'Login with GitHub', '/auth/github'%>
    <br>
    <p> Don't have an account? <%= link_to "Signup", '/signup' %> </p>
    
